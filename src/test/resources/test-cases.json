/*
  [The test code enables Jackson's ALLOW_COMMENTS feature, so comments are
  allowed in this json file.]

  The values that make up a test case:

  - description: required
  - id: required; string value of the ID
  - idtype: optional, default is "aiid"; ID type
  - informat: required: report/format of the input for this test case
  - transform: required: must match a value in main/resources/xslt/transforms.json
  - validator: required: base name of a schematron file
*/

[
  {
    "description": "NXML identity",
    "id": "31",
    "informat": "nxml",
    "transform": "identity",
    "outformat": "xml",
    "validator": "nxml"
  },
  {
    "description": "NXML to PubOne",
    "id": "31",
    "informat": "nxml",
    "transform": "pub-one",
    "outformat": "xml",
    "validator": "31-pub1"
  },
  {
    "description": "PubOne to citeproc-json",
    "id": "30",
    "informat": "pub1",
    "transform": "pub-one2json",
    "outformat": "json",
    "validator": "30-json"
  },

  {
    "description": "styled citation, default style modern-language-association",
    "id": "30",
    "informat": "pub1",
    "transform": "pub-one2ris",
    "outformat": "text",
    "validation-expressions": [
      "TY  - JOUR"
    ]
  }



]

