        <title-group>
          <article-title>A REVIEW OF LABORATORY DIAGNOSTICS FOR <italic>CHLAMYDIA TRACHOMATIS</italic> SEROVAR L, ASSOCIATED WITH LYMPHOGRANULOMA VENEREUM (LGV)</article-title>
        </title-group>
        <contrib-group>
          <contrib contrib-type="author">
            <name>
              <surname>Bernard</surname>
              <given-names>K</given-names>
            </name>
