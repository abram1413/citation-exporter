        <title-group>
          <article-title>THE USE OF ANTIBIOTIC BONE CEMENT IN PRIMARY JOINT ARTHROPLASTIES</article-title>
        </title-group>
        <contrib-group>
          <contrib contrib-type="author">
            <name>
              <surname>Sabuda</surname>
