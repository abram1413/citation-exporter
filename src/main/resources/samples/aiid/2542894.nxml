                <td align="center" rowspan="1" colspan="1">19</td>
                <td align="center" rowspan="1" colspan="1">16</td>
                <td align="center" rowspan="1" colspan="1">58</td>
              </tr>
              <tr>
                <td align="left" rowspan="1" colspan="1">75&#x02013;79%</td>
