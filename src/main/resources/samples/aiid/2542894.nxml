          <article-title>VALIDATION OF RT-PCR METHODS FOR MOLECULAR DIAGNOSTICS AND EPIDEMIOLOGY OF MEASLES VIRUS</article-title>
        </title-group>
        <contrib-group>
          <contrib contrib-type="author">
            <name>
              <surname>Hiebert</surname>
              <given-names>J</given-names>
            </name>
